<h1><%= current_admin.district.name %></h1>
<%= link_to 'Write a New Notification', new_notification_path %>
<h2>Past Notifications</h2>
<table>
  <tr>
    <th>Message</th>
    <th>Sent</th>
  </tr>

<% @notifications.each do |notification| %>
  <tr>
    <td><%= notification.message %></td>
    <td><%= notification.sent_at %></td>
  </tr>
<% end %>
</table>

<br />


